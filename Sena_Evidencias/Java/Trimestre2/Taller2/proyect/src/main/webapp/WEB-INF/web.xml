<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcom-file-list>
    <welcom-file>index.jsp</welcom-file>
  </welcom-file-list>
  <servlet>
  <servlet-name>controller.usuario</servlet-name>
  <servlet-class>controller.usuario</servlet-class>
  </servlet>

  <servlet-mapping>
  <servlet-name>controller.usuario</servlet-name>
  <url-pattern>/usuario</url-pattern>
  </servlet-mapping>

  <servlet>
  <servlet-name>controller.publicacion</servlet-name>
  <servlet-class>controller.publicacion</servlet-class>
  </servlet>

  <servlet-mapping>
  <servlet-name>controller.publicacion</servlet-name>
  <url-pattern>/publicacion</url-pattern>
  </servlet-mapping>


</web-app>
